  Questão 3

#include <stdio.h>
#include <stdlib.h>


int main() {

 // Criando variáveis
 
   float altura;
   float base;
   
 // Exibindo variáveis
 
    printf("Qual a base do seu retangulo? ");
      scanf("%f",&base,"\n");
    
    printf("Qual a altura? ");
      scanf("%f",&altura,"\n");
      
    float area = base*altura;
    
    printf("A area do seu retangulo: %f",area);
return 0;
}
