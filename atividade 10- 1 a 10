   Questão 4
#include <stdio.h>
#include <stdlib.h>


int main() {

 // Criando variáveis
 
   float num1;
   float num2;
   
  // Exibindo variáveis
  
    printf ("Qual o primeiro valor? ");
      scanf("%f",&num1,"\n");
    
    printf("Qual o segundo valor? ");
      scanf("%f",&num2,"\n");
	  
	float diferenca = num1-num2;
	
	printf("A diferenca dos dois numeros e: %f",diferenca);   

return 0;
}

   Questão 5

#include <stdio.h>
#include <stdlib.h>

int main (){

//Criando variáveis 

   float numero1;
   float numero2;

  //Exibindo variáveis

printf("Digite o primeiro valor: ");
   scanf("%f",&numero1,"\n");

printf("Digite o segundo valor: ");
   scanf("%f",&numero2,"\n");

   float valor = numero1/numero2;

printf("A divisao dos dois numeros e: %f",valor);

return 0;

}

